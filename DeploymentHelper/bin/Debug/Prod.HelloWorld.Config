<?xml version="1.0" encoding="utf-8" ?>
<appConfig environmentName="Prod" appName="HelloWorld" appLocation="HelloWorld.config">
  <settings>
    <setting id="1" key="/configuration/connectionStrings/add" attributeName="connectionString" matchExpression="(devdb1)" replaceValue="prod_db1"/>
    <setting id="2" key="/configuration/connectionStrings/add" attributeName="connectionString" matchExpression="(devdb2)" replaceValue="prod_db2"/>
    <setting id="3" key="/configuration/connectionStrings/add" attributeName="connectionString" matchExpression="(localhost)" replaceValue="127.0.0.12"/>
    <setting id="4" key="/configuration/system.serviceModel/client/endpoint[@name=&quot;MyService&quot;]" attributeName="address" replaceValue="http://127.0.0.210/PROD/MyService.svc"/>
  </settings>
</appConfig>